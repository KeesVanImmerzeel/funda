url <- "https://www.funda.nl/koop/gemeente-doesburg/"
nr_of_pages <- function(url) {
x <- xml2::read_html(url) %>%
rvest::html_node(".pagination")  %>%
rvest::html_text() %>%
gsub("\r\n", "", .) %>%
gsub(" ", "", .) %>%
gsub("Pagina", " ", .)  %>%
gsub("van", " ", .)  %>%
gsub("Volgende", "", .) %>%
gsub("^ ", "", .) %>%
gsub("  ", " ", .) %>%
strsplit(., " ") %>%
unlist() %>%
as.numeric() %>%
max(.)
return(x)
}
nr_of_pages(url)
